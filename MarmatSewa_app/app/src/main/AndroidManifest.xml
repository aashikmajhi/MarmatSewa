        <activity android:name=".GarageDashboard.garageDashboard" />
        <activity android:name=".Registration.workshopRegistration" />

        <activity android:name=".Registration.userRegistration" />
        <activity android:name=".Registration.RegistrationType" />
        <activity android:name=".Registration.SignupActivity" />
        <activity android:name=".Registration.LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>