        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MarmatSewa"
        android:usesCleartextTraffic="true">

        <activity android:name=".Dashboard.TwoWheelerServices"></activity>
        <activity android:name=".Dashboard.FourWheelerServices"></activity>
